{
  "name": "status-bar-breadcrumb",
  "displayName": "BreadCrumb in StatusBar",
  "description": "Temporary solution which helps to watch current active tab and easily navigate through relative paths and files",
  "version": "0.0.1",
  "publisher": "danields",
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "status-bar-breadcrumb.showSameLevelFilesForGiven",
        "title": "BeadCrumb: Show Files Which Relative For Given"
      }
    ],
    "configuration":[
      {
        "title": "Stats Bar Breadcrumb configuration",
        "type": "object",
        "properties": {
          "status-bar-breadcrumb.filesNavigationMenuExcludePatterns": {
            "type": "array",
            "description": "Exclude files which match given patterns from files navigation menu",
            "default": [
              ".git"
            ]
          }
        }
      }
    ]
  },
  "scripts": {
    "compile": "babel src --out-dir out --source-maps",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.6.1",
    "mocha": "^3.5.0",
    "typescript": "^2.5.2",
    "vscode": "^1.1.5"
  }
}
